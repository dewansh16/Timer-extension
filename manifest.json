{
    "manifest_version": 2,
    "name": "Timer",
    "description": "Timer app",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "app.js"
            ]
        }
    ],
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
    ]
}